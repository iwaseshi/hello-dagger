{
  "name": "hello-dagger",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "pipeline/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "fix": "turbo run fix",
    "test": "turbo run test",
    "ci": "yarn workspace @projects/cicd run ci",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "^1.4.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": "please_use_yarn_instead"
  },
  "packageManager": "yarn@1.22.19"
}
